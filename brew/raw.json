[
  {
    "name": "rhel-raw-azure",
    "version": "9.6",
    "distro": "rhel-9.6",
    "image_types": [
      "azure-rhui"
    ],
    "target": "guest-rhel-9.6.0-image",
    "arches": [
      "x86_64"
    ],
    "repo": [
      "BaseOS",
      "AppStream",
      "https://download.devel.redhat.com/devel/stratosphere-rhui-sync/rhui-microsoft-azure-rhel9/"
    ],
    "customizations": {
      "packages": [
        "rhui-azure-rhel9",
        "redhat-cloud-client-configuration",
        "system-reinstall-bootc"
      ],
      "rpm": {
        "import_keys": {
          "files": [
            "/etc/pki/rpm-gpg/RPM-GPG-KEY-microsoft-azure-release"
          ]
        }
      },
      "disk": {
        "minsize": "32 GiB",
        "partitions": [
          {
            "type": "plain",
            "mountpoint": "/",
            "minsize": "32 GiB",
            "fs_type": "xfs"
          }
        ]
      }
    }
  },
  {
    "name": "rhel-raw-azure",
    "version": "10.0",
    "distro": "rhel-10.0",
    "image_types": [
      "azure-rhui"
    ],
    "target": "guest-rhel-10.0-image",
    "arches": [
      "x86_64"
    ],
    "repo": [
      "BaseOS",
      "AppStream",
      "https://download.devel.redhat.com/devel/stratosphere-rhui-sync/rhui-microsoft-azure-rhel10/"
    ],
    "subvariant": "azure-raw",
    "customizations": {
      "packages": [
        "rhui-azure-rhel10",
        "redhat-cloud-client-configuration",
        "system-reinstall-bootc"
      ],
      "rpm": {
        "import_keys": {
          "files": [
            "/etc/pki/rpm-gpg/RPM-GPG-KEY-microsoft-azure-release"
          ]
        }
      },
      "disk": {
        "minsize": "32 GiB",
        "partitions": [
          {
            "type": "plain",
            "mountpoint": "/",
            "minsize": "32 GiB",
            "fs_type": "xfs"
          }
        ]
      }
    }
  }
]
